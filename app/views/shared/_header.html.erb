<% if logged_in? %>
  <header>
    <div class="loggedin-header-container">
        <ul>
          <li>
            <%= link_to image_tag("P_Tx7ZkSygyAMwL1542268159_1542268190", alt:"logoマーク", class:"logo-stuni"), root_path %>
          </li>
          <li>
            <%= link_to search_tweetpost_path, class:"link-clear header-icon1", method: :get do %>
              <i class="fas fa-search"></i>
            <% end %>
          </li>
          <li>
            <%= link_to user_path(current_user), class:"link-clear header-icon2", method: :get do %>
              <i class="fas fa-paw"></i>
            <% end %>
          </li>
          
        </ul>
    </div>
  </header>
<% else %>
  <header>
    <div class="notloggedin-header-container">
        <ul>
          <li><%= link_to image_tag("P_Tx7ZkSygyAMwL1542268159_1542268190", alt:"logoマーク", class:"logo-stuni"), root_path %></li>
          <li><%= link_to "ログインする", login_path, class:"header-path" %></li>
          <li><%= link_to "新規会員登録", signup_path, class:"header-path" %></li>
        </ul>
      </div>
    </div>
  </header>
<% end %>
